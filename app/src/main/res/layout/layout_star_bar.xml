<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/ll_starbar"
    android:orientation="horizontal"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content">
    <ImageView
        android:id="@+id/iv_star_01"
        android:layout_width="15dp"
        android:layout_height="15dp"
        android:layout_marginRight="5dp"
        android:background="@drawable/img_icon_star_sel"/>
    <ImageView
        android:id="@+id/iv_star_02"
        android:layout_width="15dp"
        android:layout_height="15dp"
        android:layout_marginRight="5dp"
        android:background="@drawable/img_icon_star_nor"/>
    <ImageView
        android:id="@+id/iv_star_03"
        android:layout_width="15dp"
        android:layout_height="15dp"
        android:layout_marginRight="5dp"
        android:background="@drawable/img_icon_star_nor"/>
    <ImageView
        android:id="@+id/iv_star_04"
        android:layout_width="15dp"
        android:layout_height="15dp"
        android:layout_marginRight="5dp"
        android:background="@drawable/img_icon_star_nor"/>
    <ImageView
        android:id="@+id/iv_star_05"
        android:layout_width="15dp"
        android:layout_height="15dp"
        android:layout_marginRight="5dp"
        android:background="@drawable/img_icon_star_nor"/>
</LinearLayout>